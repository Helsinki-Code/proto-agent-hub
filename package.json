{
  "name": "agentic-ai-website",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "description": "AgenticAI - Transform Your Business with Intelligent Automation",
  "scripts": {
    "dev": "vite",
    "api": "node api/contact.js",
    "start": "concurrently \"npm run api\" \"npm run dev\"",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:production": "NODE_ENV=production tsc && vite build --mode production",
    "clean": "rimraf dist siteground-deploy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "deploy": "node scripts/deploy-to-siteground.js",
    "deploy:siteground": "npm run deploy",
    "deploy:full": "npm run clean && npm run deploy",
    "test": "echo 'No tests specified yet'",
    "verify-build": "npm run build && ls -la dist/assets/"
  },
